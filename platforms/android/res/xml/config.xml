<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pisa.gampangpoll" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GooglePlus">
        <param name="android-package" value="nl.xservices.plugins.GooglePlus" />
        <param name="onload" value="true" />
    </feature>
    <access origin="https://accounts.google.com/*" />
    <feature name="GoogleConnectPlugin">
        <param name="android-package" value="com.cabot.plugins.googleplus.ConnectGoogle" />
    </feature>
    <allow-intent href="market:*" />
    <name>GampangPoll</name>
    <description>
        Audience Response System For Web and Android
    </description>
    <author email="" href="">
        Pisa Industries
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
